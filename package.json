{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 5173",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "format": "prettier --write . --ignore-unknown",
    "lint": "next lint . --no-cache",
    "format:lint": "bun run type-check && bun run format && bun run lint",
    "prepare": "husky"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.10.2",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "iconv-lite": "^0.6.3",
    "jotai": "^2.11.1",
    "lucide-react": "^0.475.0",
    "metadata-scraper": "^0.2.61",
    "next": "15.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^22.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.4",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "sass": "^1.82.0",
    "typescript": "^5",
    "typescript-eslint": "^8.21.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "bunx tsc-files --noEmit",
    "*.{js,jsx,ts,tsx,js,json,md}": "bun run format && bun run lint"
  }
}
